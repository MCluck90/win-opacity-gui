{
	"extends": "eslint:recommended",
	"plugins": [
		"react"
	],
	"parserOptions": {
		"ecmaVersion": 2015,
		"sourceType": "module",
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": true,
			"spread": true
		}
	},
	"env": {
		"node": true,
		"browser": true
	},
	"rules": {
		"eqeqeq": "error",
		"react/jsx-uses-react": "error",
		"react/jsx-uses-vars": "error"
	}
}